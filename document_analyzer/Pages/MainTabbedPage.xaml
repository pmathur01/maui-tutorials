<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="document_analyzer.Pages.MainTabbedPage"
            Title="MainPage">
    <TabbedPage.Children>
        <ContentPage Title="Home">
            <StackLayout Padding="30">
                <Label Text="Welcome to the Home Page" FontSize="Large" HorizontalOptions="Center" />
            </StackLayout>
        </ContentPage>
        <ContentPage Title="Profile">
            <StackLayout Padding="30">
                <Label Text="This is the Profile Page" FontSize="Large" HorizontalOptions="Center" />
            </StackLayout>
        </ContentPage>
        <ContentPage Title="Settings">
            <StackLayout Padding="30">
                <Label Text="Settings Page" FontSize="Large" HorizontalOptions="Center" />
            </StackLayout>
        </ContentPage>
    </TabbedPage.Children>
</TabbedPage>
